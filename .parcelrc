{
  "extends": [
    "@parcel/config-default"
  ],
  "reporters": [
    "...",
    "parcel-reporter-static-files-copy"
  ],
  "transformers": {
    "*.glsl": [
      "...",
      "@parcel/transformer-glsl"
    ],
    "*.scss": [
      "...",
      "@parcel/transformer-sass"
    ]
  },
  "optimizers": {
    "*.js": [
      "...",
      "@parcel/optimizer-terser"
    ]
  }
}